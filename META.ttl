	dc:issued         "2012-10-05"^^xsd:date;
	dcs:changeset     [
		dcs:item [
			a dcs:Change;
			rdfs:label "Replace B::Hooks::* usage with Hook::AfterRuntime.";
		];
		dcs:item [
